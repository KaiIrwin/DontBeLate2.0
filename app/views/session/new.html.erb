<h1>Log in / log out 関係 sessions</h1>

<%= form_with url: sign_in_path do |form| %>
<div class="mb-3">
  <%= form.text_field :email, class:"form-control"%>
</div>
<div class="mb-3">
  <%= form.password_field :password, class:"form-control" %>
</div>
<div class="mb-3">
  <%= form.submit "Log in", class:"btn btn-primary" %>
</div>
<%end%>

